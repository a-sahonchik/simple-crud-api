{
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "root": true,
  "rules": {
    "indent": "off",
    "@typescript-eslint/indent": ["error", 4],
    "import/prefer-default-export": "off",
    "import/no-default-export": "error"
  }
}
